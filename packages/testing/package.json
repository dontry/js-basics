    "flow-bin": "^0.93.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-emotion": "^10.0.9",
    "jest-worker": "^24.4.0",
    "lint-staged": "^8.1.5",
