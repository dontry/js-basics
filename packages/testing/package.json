{
  "name": "testing",
  "version": "1.0.0",
  "description": "testing javascript",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node --require ./setup-global.js index.js",
    "commit": "git cz",
    "lint": "eslint src",
    "prettier": "./node_modules/.bin/prettier  \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\"",
    "format": "npm run prettier -- --write",
    "validate-staged": "lint-staged && npm run test",
    "validate": "npm run lint & npm run prettier -- --list-different && npm run flow",
    "flow": "flow"
  },
  "keywords": [
    "test"
  ],
  "author": "dontry",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "flow-bin": "^0.93.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4"
  },
  "config": {
    "commitizen": {
      "path": "./packages/testing/node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "lint-staged": "^8.1.5"
  }
}
