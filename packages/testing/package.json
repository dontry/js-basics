    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "assert": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-emotion": "^10.0.9",
    "create-jest-runner": "^0.5.0",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.14.1",
